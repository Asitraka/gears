/random/setSeeds 2412 79628
/output/file AmBe.root

# must be set before /run/initialize
/geometry/source geom/NICE/CsI2in10cm.CF6in.tg
/physics_lists/hadron/elastic true
/physics_lists/hadron/inelastic true
/run/initialize

# print volume list
/vis/ASCIITree/verbose 13
/vis/drawTree
/geometry/test/run

# AmBe neutron spectrum
/control/execute macro/src/AmBe.mac
/gps/pos/centre 0 0 -22.5 cm

/run/beamOn 10
